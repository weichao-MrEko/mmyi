<view><view style="background-image:url('../../../static/img/userbg.jpg');background-size:cover;"><block wx:if="{{showHeader}}"><view class="status" style="{{'position:'+(headerPosition)+';'+('top:'+(statusTop)+';')}}"></view></block><block wx:if="{{showHeader}}"><view class="header" style="{{'position:'+(headerPosition)+';'+('top:'+(headerTop)+';')}}"><view class="addr"></view><view class="input-box"></view><view class="icon-btn"><view data-event-opts="{{[['tap',[['',['$event']]]]]}}" class="icon " bindtap="__e"></view><view data-event-opts="{{[['tap',[['toSetting',['$event']]]]]}}" class="icon setting" bindtap="__e"></view></view></view></block><block wx:if="{{showHeader}}"><view class="place"></view></block><view class="user"><view class="left"><image src="{{user.avatar}}" data-event-opts="{{[['tap',[['toPersonal',['$event']]]]]}}" bindtap="__e"></image></view><view class="right"><view data-event-opts="{{[['tap',[['toLogin',['$event']]]]]}}" class="username" bindtap="__e">{{user.name}}</view><view data-event-opts="{{[['tap',[['toSetting',['$event']]]]]}}" class="signature" bindtap="__e">{{user.sign}}</view></view></view></view><view class="toolbar"><view class="title">{{jsonData.toolbar}}</view><view class="list"><block wx:for="{{jsonData.mytoolbarList}}" wx:for-item="row" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['toPage',['$0','$1'],[[['jsonData.mytoolbarList','',index,'url']],[['jsonData.mytoolbarList','',index,'id']]]]]]]}}" class="box" bindtap="__e"><view class="img"><image src="{{row.img}}"></image></view><view class="text">{{row.text}}</view><view class="icon xiangyou" style="font-size:28rpx;margin:0 20rpx;"></view></view></block></view></view><view class="toolbar" style="padding:0;"><view class="list"><block wx:for="{{jsonData.shopbarList}}" wx:for-item="row" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['toPage',['$0','$1'],[[['jsonData.shopbarList','',index,'url']],[['jsonData.shopbarList','',index,'id']]]]]]]}}" class="box" bindtap="__e"><view class="img"><image src="{{row.img}}"></image></view><view class="text">{{row.text}}</view><view class="icon xiangyou" style="font-size:28rpx;margin:0 20rpx;"></view></view></block></view></view><view class="place-bottom"></view></view>